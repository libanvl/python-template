{
    "include": [
      "pkg",
      "test"
    ],
    
    "exclude": [
      "**/node_modules",
      "**/__pycache__",
      "src/experimental",
      "src/typestubs"
    ],
  
    "ignore": [
      "**/*.pyi"
    ],
  
    "stubPath": "typings",
  
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
  
    "pythonVersion": "3.8",
    "pythonPlatform": "Linux"
}